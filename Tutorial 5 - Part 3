//Tutorial 5 - Part 3

//Include the Servo Library
//Create a Servo Object
#include <Servo.h>
Servo jessicasServo;

//Define the Pins
int servoPin = 9;
int distPin = 0;

void setup()
{
  
  //"Attach" The Servo Object
  //We don't need to set it as an output annually
  jessicasServo.attach(servoPin);
  
}

void loop()
{
  int dist = analogRead(distPin)
  int pos = map(dist, 0, 1023, 0, 180);

    jessicasServo.write(i);

}
